{
  "name": "conniebot",
  "version": "3.1.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xsduan/conniebot.git"
  },
  "scripts": {
    "lint": "tslint -p . -c tslint.json -e './**/*.json'",
    "fix": "tslint --fix -p . -c tslint.json -e './**/*.json'",
    "start": "nodemon --exitcrash --ignore *.sqlite -x ts-node index.ts",
    "forever": "forever start --uid conniebot --killSignal=SIGTERM -a -c \"npm start\" ./",
    "test": "npm run lint"
  },
  "description": "Does various language stuff.",
  "main": "bot.js",
  "author": "Shane Duan",
  "dependencies": {
    "@types/config": "0.0.34",
    "@types/js-yaml": "^3.11.2",
    "@types/node": "^10.7.1",
    "@types/sqlite3": "^3.1.3",
    "@types/xregexp": "^3.0.29",
    "config": "^1.30.0",
    "discord.js": "^11.3.2",
    "forever": "^0.15.3",
    "js-yaml": "^3.11.0",
    "nodemon": "^1.18.3",
    "sql-template-strings": "^2.2.2",
    "sqlite": "^3.0.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1",
    "xregexp": "^4.1.1"
  },
  "devDependencies": {
    "standard": "^11.0.1",
    "tslint": "^5.11.0"
  }
}
