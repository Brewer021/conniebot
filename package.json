{
  "name": "conniebot",
  "version": "3.1.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xsduan/conniebot.git"
  },
  "scripts": {
    "lint": "tslint -p . -c tslint.json -e './**/*.json'",
    "fix": "tslint --fix -p . -c tslint.json -e './**/*.json'",
    "build": "node build",
    "start": "node build -s",
    "forever": "node build -f",
    "test": "npm run lint"
  },
  "description": "Does various language stuff.",
  "main": "bot.js",
  "author": "Shane Duan",
  "dependencies": {
    "config": "^1.30.0",
    "discord.js": "^11.3.2",
    "js-yaml": "^3.11.0",
    "npmlog": "^4.1.2",
    "sql-template-strings": "^2.2.2",
    "sqlite": "^3.0.0",
    "xregexp": "^4.1.1"
  },
  "devDependencies": {
    "@types/config": "0.0.34",
    "@types/js-yaml": "^3.11.2",
    "@types/node": "^10.7.1",
    "@types/xregexp": "^3.0.29",
    "minimist": "^1.2.0",
    "standard": "^11.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "nodemonConfig": {
    "ignore": [
      "*.sqlite"
    ],
    "exitcrash": true
  }
}
